{
  "name": "express-enhanced-logger",
  "version": "1.0.0",
  "description": "An enhanced Express.js logger with performance monitoring, SQL query formatting, and customizable features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "preversion": "npm run build && npm test"
  },
  "keywords": [
    "express",
    "logger",
    "winston",
    "sql",
    "prisma",
    "performance",
    "middleware"
  ],
  "author": "Dylan Dietz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deetss/express-enhanced-logger.git"
  },
  "bugs": {
    "url": "https://github.com/Deetss/express-enhanced-logger/issues"
  },
  "homepage": "https://github.com/Deetss/express-enhanced-logger#readme",
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.5",
    "conventional-changelog-cli": "^4.1.0",
    "jest": "^30.2.0",
    "semantic-release": "^24.2.9",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "dist/**/*.js",
      "!dist/**/*.d.ts"
    ],
    "coverageDirectory": "coverage"
  }
}
